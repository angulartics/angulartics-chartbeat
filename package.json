    "plugin",
    "angular",
    "angulartics"
  ],
  "author": "Sergio Acosa <acosta.checho@simian.co> (https://github.com/chechoacosta)",
  "contributors": [
    "Tim Elfelt <timelf123@gmail.com> (https://github.com/timelf123)",
    "Luis Farzati <lfarzati@gmail.com> (https://github.com/luisfarzati)"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist",
